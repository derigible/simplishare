{
  "name": "budgetr_web",
  "version": "1.0.0",
  "description": "My budgeting app's frontend",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "start:production": "cross-env NODE_ENV=production PRODUCTIONTEST=1 npm run build && http-server __build__ -p 8001",
    "build": "npm run clean && NODE_ENV=production webpack && cp __build__/index.html ../public/index.html",
    "clean": "rm -rf __build__",
    "bundle-analyze": "ANALYZE_BUNDLE=1 webpack --profile --json > stats.json",
    "bundle-analyze-production": "NODE_ENV=production ANALYZE_BUNDLE=1 webpack --profile --json > prod-stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://pmarca@bitbucket.org/pmarca/budgetr_web.git"
  },
  "keywords": [],
  "author": "Marc Phillips",
  "license": "ISC",
  "homepage": "https://bitbucket.org/pmarca/budgetr_web#readme",
  "devDependencies": {
    "@instructure/ui-codemods": "^5.30.0",
    "babel-plugin-recharts": "^1.2.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "compression-webpack-plugin": "^1.1.12",
    "cross-env": "^5.2.0",
    "dotenv": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "react-hot-loader": "^1.3.1",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "script-loader": "^0.7.0",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.11.3"
  },
  "dependencies": {
    "@instructure/ui-a11y": "^5.31.0",
    "@instructure/ui-alerts": "^5.31.0",
    "@instructure/ui-billboard": "^5.31.0",
    "@instructure/ui-buttons": "^5.31.0",
    "@instructure/ui-elements": "^5.31.0",
    "@instructure/ui-forms": "^5.31.0",
    "@instructure/ui-icons": "^5.31.0",
    "@instructure/ui-layout": "^5.31.0",
    "@instructure/ui-overlays": "^5.31.0",
    "@instructure/ui-presets": "^5.31.0",
    "@instructure/ui-themeable": "^5.31.0",
    "@instructure/ui-themes": "^5.31.0",
